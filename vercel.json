{
    "version": 2,
    "projects": [
      {
        "rootDirectory": "nextjs-dashboard"
      }
    ],
    "builds": [
      {
        "src": "nextjs-dashboard/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "nextjs-dashboard/$1"
      }
    ]
  }
  